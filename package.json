{
  "name": "@wlvyr/common",
  "version": "0.1.1",
  "description": "Web JS common tools",
  "author": "wlvyr",
  "license": "MIT",
  "keywords": [
    "wlvyr",
    "request",
    "queue",
    "queue-request",
    "sync",
    "local-storage",
    "localstorage"
  ],
  "main": "./dist/common/index.cjs",
  "module": "./dist/common/index.js",
  "types": "./dist/types/common/index.d.ts",
  "react-native": "./dist/common/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/common/index.js",
      "require": "./dist/common/index.cjs",
      "types": "./dist/types/common/index.d.ts"
    },
    "./error": {
      "import": "./dist/error/index.js",
      "require": "./dist/error/index.cjs",
      "types": "./dist/types/error/index.d.ts"
    },
    "./network": {
      "import": "./dist/network/index.js",
      "require": "./dist/network/index.cjs",
      "types": "./dist/types/network/index.d.ts"
    },
    "./async": {
      "import": "./dist/async/index.js",
      "require": "./dist/async/index.cjs",
      "types": "./dist/types/async/index.d.ts"
    },
    "./security": {
      "import": "./dist/security/index.js",
      "require": "./dist/security/index.cjs",
      "types": "./dist/types/security/index.d.ts"
    },
    "./storage": {
      "import": "./dist/storage/index.js",
      "require": "./dist/storage/index.cjs",
      "types": "./dist/types/storage/index.d.ts"
    },
    "./sync": {
      "import": "./dist/sync/index.js",
      "require": "./dist/sync/index.cjs",
      "types": "./dist/types/sync/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "dist/types/",
    "README.md",
    "LICENSE.txt"
  ],
  "scripts": {
    "build": "rollup -c ./config/rollup.config.js",
    "build-typedef": "npx tsc --emitDeclarationOnly",
    "build-all": "npm run build && npm run build-typedef",
    "test": "node ./node_modules/jest/bin/jest --verbose --detectOpenHandles --config=./config/jest.config.js",
    "test-no-cache": "node ./node_modules/jest/bin/jest --no-cache --verbose --detectOpenHandles --config=./config/jest.config.js",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest --runInBand --no-cache --config=./config/jest.config.js",
    "test-clear-cache": "./node_modules/jest/bin/jest.js --clearCache"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.14",
    "@types/libsodium-wrappers-sumo": "^0.7.8",
    "jest": "^29.7.0",
    "rollup": "^4.41.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "libsodium-wrappers-sumo": "^0.7.15"
  }
}
